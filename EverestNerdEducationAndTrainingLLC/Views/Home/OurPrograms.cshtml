                            <div class="col-lg-4 col-md-6">
                            <div class="singel-course mt-30">
                                <div class="thum">
                                    <div class="image">
                                        <img src="@item.ImagePath" alt="Course">
                                    </div>
                                    <div class="price">
                                        <span>Free</span>
                                    </div>
                                </div>
                                <div class="cont">
                                    <a href="#"><h4>@item.Name</h4></a>
                                    <div class="course-teacher">
                                        <div class="thum">
                                            <a href="#"><img src="@item.TeacherIconImagePath" alt="teacher"></a>
                                        </div>
                                        <div class="name">
                                            <a href="#"><h6>@item.Teacher</h6></a>
                                        </div>
                                    </div>
                                </div>
                            </div> <!-- singel course -->
                        </div>    
                        }  
                        }
                        


